    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "def get_director(crew):\n",
        "  for crew_member in crew:\n",
        "    if crew_member['job'] == \"Director\":\n",
        "      return crew_member['name']\n",
        "  return np.nan\n",
        "result_df['director'] = result_df['crew'].apply(get_director)\n",
        "result_df[['original_title','director']].head(10)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 363
        },
